function [next_guess_solution,best_price,best_cut] = DecideTheCurrSolution(temprature,delta_price, current_solution,next_guess_solution,best_price)
    if delta_price > 0 || rand() < exp(-delta_price/temprature)
       next_guess_solution = current_solution; 
       price_current = calc_price(next_guess_solution);
    end

    if price_current > best_price
        best_price = price_current;
        best_cut = current_solution;
    end 

end
